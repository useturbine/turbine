---
import logo from "assets/logo.svg";
// import { Image } from 'astro:assets';
import ThemeSelector from "@components/themeselector.astro";
import { menuitems } from "@components/navbar/navbar.astro";
---

<footer class="pb-12 pt-28 dark:bg-stone-950">
	<div class="mx-auto max-w-6xl px-6 md:px-12 lg:px-6 xl:px-0">
		<ul role="list" class="flex flex-wrap items-center justify-center gap-4 py-4 text-gray-600 dark:text-white sm:gap-8">
			<li role="listitem">
				<a href="/" class="duration-300 hover:text-gray-900 dark:hover:text-gray-100">Home</a>
			</li>
			{
				menuitems.map(
					(item) =>
						item &&
						item.path && (
							<li role="listitem">
								<a href={item.path} class="duration-300 hover:text-gray-900 dark:hover:text-gray-100">
									{item.title}
								</a>
							</li>
						),
				)
			}
		</ul>
		<div class="m-auto mt-4 flex w-max items-center justify-between space-x-4 text-gray-500 dark:text-white">
			<a
				class="duration-300 hover:text-gray-900 dark:hover:text-gray-100"
				href="https://join.slack.com/t/turbinespace/shared_invite/zt-24haihf2d-Mu71smnsyjb~mschxugjVQ
			"
				title="slack"
				target="_blank"
				aria-label="slack">
				<svg xmlns="http://www.w3.org/2000/svg" fill="currentColor" class="m-auto w-5" viewBox="0 0 512 512">
					<path d="M126.12,315.1A47.06,47.06,0,1,1,79.06,268h47.06Z"></path>
					<path d="M149.84,315.1a47.06,47.06,0,0,1,94.12,0V432.94a47.06,47.06,0,1,1-94.12,0Z"></path>
					<path d="M196.9,126.12A47.06,47.06,0,1,1,244,79.06v47.06Z"></path>
					<path d="M196.9,149.84a47.06,47.06,0,0,1,0,94.12H79.06a47.06,47.06,0,0,1,0-94.12Z"></path>
					<path d="M385.88,196.9A47.06,47.06,0,1,1,432.94,244H385.88Z"></path>
					<path d="M362.16,196.9a47.06,47.06,0,0,1-94.12,0V79.06a47.06,47.06,0,1,1,94.12,0Z"></path>
